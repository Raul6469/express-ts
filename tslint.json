{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "variable-name": [false],
    "no-shadowed-variable": [false],
    "no-var-requires": [false],
    "no-console": [false],
    "object-literal-sort-keys": [false],
    "no-namespace": [false],
    "interface-name": [false]
  },
  "rulesDirectory": []
}